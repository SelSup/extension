{
  "manifest_version": 3,
  "version": "0.0.1",
  "name": "SelSup",
  "description": "__MSG_description__",
  "default_locale": "ru",
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions" : [
    "http://*/*",
    "https://*/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "overwrite-origin",
        "enabled": true,
        "path": "rules/overwrite-origin.json"
      },
      {
        "id": "allow-credentials",
        "enabled": true,
        "path": "rules/allow-credentials.json"
      },
      {
        "id": "allow-headers",
        "enabled": true,
        "path": "rules/allow-headers.json"
      },
      {
        "id": "referer",
        "enabled": true,
        "path": "rules/referer.json"
      },
      {
        "id": "csp",
        "enabled": true,
        "path": "rules/csp.json"
      }
    ]
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },
  "homepage_url": "https://selsup.ru/extensions/"
}
